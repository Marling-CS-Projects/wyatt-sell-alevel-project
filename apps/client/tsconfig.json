{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"jsx": "preserve",
		"allowJs": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"isolatedModules": true,
		"baseUrl": "./",
		"paths": {
			"react-leaflet-draw": ["react-leaflet-draw-override"]
		}
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"],
}
